{
  "expo": {
    "name": "Friday Calm",
    "slug": "Friday Calm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7D7E3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gofriday.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#F7D7E3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gofriday.app"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      }
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "gofriday",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://gofriday.shop"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  },
  "scheme": "Friday Calm"
}