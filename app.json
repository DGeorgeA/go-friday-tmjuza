
{
  "expo": {
    "name": "Friday Calm",
    "slug": "Friday Calm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7D7E3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gofriday.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#F7D7E3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gofriday.app"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "scheme": "gofriday",
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
